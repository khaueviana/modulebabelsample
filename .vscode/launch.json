{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch App.js",
            "program": "${workspaceRoot}/src/index.js",
            "outFiles": [
                "${workspaceRoot}/.compiled/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Current File",
            "program": "${file}",
            "outFiles": [
                "${workspaceRoot}/.compiled/**/*.js"
            ]
        }
    ]
}